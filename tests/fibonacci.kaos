// Fibonacci number generators

// Space optimized method

num def return_var(num x)
    return x
end

num def fibo1(num n)
    num a = 0
    num b = 1
    num c = 0
    n - 1 times do
        c = a + b
        a = b
        b = c
    end
end {
    n == 0  : return_var(a),
    n == 1  : return_var(b),
    default : return_var(b)
}


num x = 1
20 times do
    print fibo1(x)
    x++
end
